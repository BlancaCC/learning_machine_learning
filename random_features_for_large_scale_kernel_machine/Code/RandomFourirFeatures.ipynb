    "import numpy as np\n",
    "def gaussian_fourier_tranform(w: np.array)-> float:\n",
    "    '''\n",
    "    W array \n",
    "    ''' \n",
    "    D = len(w)\n",
    "    norm = w.dot(w)\n",
    "\n",
    "    return (2*np.pi)**(-D/2)* np.exp(norm/2)\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
